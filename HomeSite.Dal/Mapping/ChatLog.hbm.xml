<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="HomeSite.Dal" namespace="HomeSite.Dal.Domain">
  <class name="ChatLog">
    <id name="ChatLogID">
      <generator class="identity"></generator>
    </id>
    <many-to-one name="ChatGroup" column="chatGroupId"/>
    <many-to-one name="Sender" column="senderId" />
    <property name="Created"/>
    <property name="Message"/>
  </class>
</hibernate-mapping>